<%= form_with(model: @supplier) do |f| %>
  <div>
    <%= f.label :corporate_name %>
    <%= f.text_field :corporate_name %>
  </div>

  <div>
    <%= f.label :brand_name %>
    <%= f.text_field :brand_name %>
  </div>

  <div>
    <%= f.label :nif %>
    <%= f.text_field :nif %>
  </div>

  <div>
    <%= f.label :full_address %>
    <%= f.text_field :full_address %>
  </div>

  <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :contact_number %>
    <%= f.text_field :contact_number %>
  </div>

  <div>
    <%= f.submit 'Enviar' %>
  </div>

<% end %>